{
    "name": "eo/airbrake-bundle",
    "type": "symfony-bundle",
    "description": "Airbrake.io integration for Symfony2",
    "keywords": ["airbrake", "exception", "errbit"],
    "license": "MIT",
    "authors": [
        {
            "name": "Eymen Gunay",
            "email": "eymen@egunay.com"
        }
    ],
    "require": {
        "symfony/symfony": ">=2.1,<2.4-dev",
