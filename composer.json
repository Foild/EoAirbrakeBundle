    },
    "target-dir": "Eo/AirbrakeBundle",
    "autoload": {
        "psr-0": {
            "Eo\\AirbrakeBundle": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}